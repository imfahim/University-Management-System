@model IEnumerable<ProEntity.Faculty>

@{
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Faculty</title>
</head>
<body align="center">
    <div align="center">
    <h2>Faculties</h2>
    <table class="table table-bordered">
        <tr>
            <th>
               <label align="center">Id</label>
            </th>
            <th>
                <label align="center">Name</label>
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FacultyId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FacultyName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "EditFac", new { id = item.FacultyId }) |
                    @Html.ActionLink("Details", "DetailsFac", new { id = item.FacultyId }) |
                    @Html.ActionLink("Delete", "DeleteFac", new { id = item.FacultyId })
                </td>
            </tr>
        }

    </table>
    <p>@Html.ActionLink("Create New Faculty", "CreateFac")</p>
    <p>@Html.ActionLink("Back", "Index")</p>
     </div>
</body>
</html>
